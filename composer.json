{
    "name": "silverstripe/installer",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": ">=5.5",
        "silverstripe/framework": "4.10.*@stable",
        "silverstripe/cms": "4.10.*@stable",
        "silverstripe/recipe-plugin": "*",
        "silverstripe/recipe-cms": "*",
        "silverstripe/reports": "*",
        "silverstripe/siteconfig": "*",
        "undefinedoffset/sortablegridfield": "*",
        "silverstripe/blog": "*",
        "silverstripe/widgets": "*",
        "jonom/focuspoint": "*",
        "jonom/silverstripe-betternavigator": "*",
        "md/saml": "dev-md",
        "md/division-analytics": "1.3.*",
        "gorriecoe/silverstripe-webfonts": "*",
        "silverstripe/sharedraftcontent": "dev-master",
        "bummzack/sortablefile": "^2.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "resources-dir": "_resources",
        "expose": [
            "themes/exchanges/dist/favicons",
            "themes/exchanges/dist/styles",
            "themes/exchanges/dist/scripts",
            "themes/exchanges/dist/images",
            "themes/exchanges/dist/fonts",
            "themes/exchanges/css"
        ],
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ]
    },
    "repositories": {
        "0": {
            "type": "vcs",
            "url": "https://github.com/jacobbuck/silverstripe-colorfulimage"
        },
        "1": {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/silverstripe-saml"
        },
        "2": {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/division-analytics"
        },
        "3": {
            "type": "vcs",
            "url": "https://github.com/PlatoCreative/silverstripe-typekit"
        },
        "repo-name": {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/deploy-helper"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
